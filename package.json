{
  "name": "wuta-community-admin",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "clean:cache": "rm -rf node_modules/.cache/ && rm -rf node_modules/.vite",
    "clean:lib": "rm -rf node_modules",
    "lint": "eslint --fix",
    "format": "prettier . --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/core": "^8.7.5",
    "ant-design-vue": "^3.2.10",
    "axios": "^0.27.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "lint-staged": {
    "*.{vue,js,ts,html}": "pnpm run format && pnpm run lint"
  },
  "homepage": "https://github.com/LiJiaPeng001/vue3-ts-vite-template",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.9.0",
    "unplugin-vue-components": "^0.21.1",
    "vite": "2.9.12",
    "vue-tsc": "^0.34.7"
  }
